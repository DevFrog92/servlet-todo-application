<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <context-param>
        <param-name>MariaDB</param-name>
        <param-value>org.mariadb.jdbc.Driver</param-value>
    </context-param>
    <context-param>
        <param-name>MariaDBURL</param-name>
        <param-value>jdbc:mariadb://localhost:3306/todo</param-value>
    </context-param>
    <context-param>
        <param-name>MariaDBUsername</param-name>
        <param-value>admin</param-value>
    </context-param>
    <context-param>
        <param-name>MariaDBPassword</param-name>
        <param-value>mariadb123</param-value>
    </context-param>
</web-app>
